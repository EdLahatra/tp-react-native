    "date-fns": "^2.16.1",
    "js-md5": "^0.7.3",
    "md5": "^2.3.0",
    "moment": "^2.29.0",
    "native-base": "^2.13.14",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-linear-layout": "^3.0.0",
